# An example controller showing a sample home page
GET     /api/ping                   controllers.Ping.ping

POST    /api/webhook                controllers.Webhook.webhook

POST    /scrap_city                 controllers.ScrappingController.scrapCity

POST    /scrap_theater              controllers.ScrappingController.scrapTheater

POST    /scrap_movie                controllers.ScrappingController.scrapMovie

GET     /actor_start                controllers.ScrappingSchedulerController.startActor

GET     /actor_stop                 controllers.ScrappingSchedulerController.stopActor

# Map static resources from the /public folder to the /assets URL path
GET     /assets/*file               controllers.Assets.versioned(path="/public", file: Asset)
